{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "strict": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "module": "es6",
        "moduleResolution": "node",
        //"target": "es5",
        "target": "es2015",
        "lib": ["es5", "es5", "es7", "ES2017", "ES2018","ES2019","ES2020",  "ES2021", "ES2022", "ESNext",  "dom"],
        "allowJs": true,
        "allowSyntheticDefaultImports": true
    },
    "include": [
        "./src/*"
    ]
}
